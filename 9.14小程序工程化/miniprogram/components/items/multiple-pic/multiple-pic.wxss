/* components/items/multiple-pic/multiple-pic.wxss */
.item {
  min-height: 42px;
  padding: 16px 0;
  margin: 0 15px;
  border-bottom: 1px solid rgba(221, 221, 221, 0.6);
}
.title {
  font-size: 17px;
  margin: 10px auto;
}

.image-list {
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}

.image-list image {
  width: 33%;
  height: 80px;
  display: inline-block;
}